/dts-v1/;

#include "rt3352.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "DCS-933L", "ralink,rt3352-soc";
	model = "D-Link DCS-933L";

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		wps {
			label = "wps";
			gpios = <&gpio0 0 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		reset {
			label = "reset";
			gpios = <&gpio0 10 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		power {
			label = "dcs-933l:red:power";
			gpios = <&gpio0 2 GPIO_ACTIVE_LOW>;
		};

		status {
			label = "dcs-933l:green:status";
			gpios = <&gpio0 9 GPIO_ACTIVE_LOW>;
		};

		wps {
			label = "dcs-933l:blue:wps";
			gpios = <&gpio0 13 GPIO_ACTIVE_LOW>;
		};
	};

	gpio-export {
		compatible = "gpio-export";
		#size-cells = <0>;

/*		light_sensor {
			gpio-export,name = "light_sensor";
			gpio-export,output = <0>;
			gpios = <&gpio0 11 GPIO_ACTIVE_LOW>;
		};

		ir_led {
			gpio-export,name = "ir_led";
			gpio-export,output = <1>;
			gpios = <&gpio0 12 GPIO_ACTIVE_LOW>;
		};
*/
		ir_cutoff_filter {
			gpio-export,name = "ir_cutoff_filter";
			gpio-export,output = <1>;
			gpios = <&gpio0 14 GPIO_ACTIVE_LOW>;
		};
	};
};

&spi0 {
	status = "okay";

	m25p80@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		linux,modalias = "m25p80";
		spi-max-frequency = <10000000>;

		partition@0 {
			label = "u-boot";
			reg = <0x0 0x30000>;
			read-only;
		};

		partition@30000 {
			label = "u-boot-env";
			reg = <0x30000 0x10000>;
			read-only;
		};

		factory: partition@40000 {
			label = "factory";
			reg = <0x40000 0x10000>;
			read-only;
		};

		partition@50000 {
			label = "firmware";
			reg = <0x50000 0x7b0000>;
		};
	};
};

&spi1 {
       status = "okay";

       spidev@0 {
               compatible = "linux,spidev";
               spi-max-frequency = <10000000>;
               reg = <0>;
       };
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "i2c", "jtag", "uartf", "led";
			ralink,function = "gpio";
		};

		rgmii {
			ralink,group = "rgmii";
			ralink,function = "rgmii";
		};

		mdio {
			ralink,group = "mdio";
			ralink,function = "mdio";
		};
	};
};

&ethernet {
	mtd-mac-address = <&factory 0x4>;
};

&esw {
	mediatek,portmap = <0x2f>;
};

&wmac {
	ralink,mtd-eeprom = <&factory 0>;
};

&ehci {
	status = "okay";
};

&ohci {
	status = "okay";
};

